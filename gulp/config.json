{
	"injectPattern": "//<-inject:<filename>->",
	"bundles": [
		{
			"base": "js/overview.js",
			"sources": [
				"js/card.js",
				"js/tabs.js",
				"css/overview.css",
				"css/overviewHeader.css",
				"css/card.css",
				"html/overview.html",
				"html/card.html"
			]
		},
		{
			"base": "js/progressive.js",
			"sources": [
				"js/ripple.js",
				"js/cardanimation.js",
				"css/ripple.css",
				"css/progressive.css",
				"css/css/cardanimation.css"
			]
		},
		{
			"base": "js/second-view.js",
			"sources": [
				"html/second-view.html",
				"css/second-view.css"
			]
		}
	],
	"files": {
		"js": [
			"js/card.js",
			"js/overview.js",
			"js/tabs.js",
			"js/progressive.js",
			"js/ripple.js",
			"js/cardanimation.js",
			"js/second-view.js"
		],
		"html": [
			"index.html",
			"html/card.html",
			"html/overview.html",
			"html/second-view.html"
		],
		"css": [
			"css/card.css",
			"css/overview.css",
			"css/overviewHeader.css",
			"css/splashdrawer.css",
			"css/splashloadinganimation.css",
			"css/splashscreen.css",
			"css/ripple.css",
			"css/progressive.css",
			"css/cardanimation.css",
			"css/second-view.css"
		],
		"img": [
			"static/logo168.png",
			"static/logo144.png",
			"static/logo192.png",
			"static/logo96.png",
			"static/logo72.png",
			"static/logo48.png"
		]
	},
	"output": {
		"index.html": "index.html",
		"bundles/overview.js": "a.js",
		"bundles/progressive.js": "b.js",
		"bundles/second-view.js": "d.js",
		"../node_modules/@webcomponents/custom-elements/custom-elements.min.js": "c.js",
		"static/*.png": "static/*.png"
	},
	"rename": {
		"bundles/overview.js": "a.js",
		"bundles/progressive.js": "b.js",
		"bundles/second-view.js": "d.js",
		"../node_modules/@webcomponents/custom-elements/custom-elements.min.js": "c.js",
		"<script src=\"classid.js\"></script>": ""
	},
	"addCopyright": [
		"index.html",
		"bundles/overview.js",
		"bundles/progressive.js",
		"bundles/second-view.js"
	]
}