{
	"injectPattern": "//<-inject:<filename>->",
	"injectPatternJS": "//<-use:<filename>->",
	"files": {
		"js": [
			"card/card.js",
			"card/cardmanager.js",
			"overview/overview.js",
			"tabs/tabs.js",
			"overview/stage-1/overview-stage-1.js",
			"overview/stage-2/overview-stage-2.js",
			"ripple/ripple.js",
			"card/cardanimation.js",
			"second-view/second-view.js",
			"info/info.js",
			"helpers/cssinject.js",
			"helpers/animationsyncer.js",
			"helpers/cubicbezier.js"
		],
		"html": [
			"index.html",
			"card/card.html",
			"overview/overview.html",
			"second-view/second-view.html",
			"tabs/tabs.html",
			"info/info.html"
		],
		"css": [
			"card/card.css",
			"overview/overview.css",
			"overview/overviewHeader.css",
			"splashscreen/splashdrawer.css",
			"splashscreen/splashloadinganimation.css",
			"splashscreen/splashscreen.css",
			"ripple/ripple.css",
			"overview/stage-1/stage-1.css",
			"card/cardanimation.css",
			"second-view/second-view.css",
			"tabs/tabs.css",
			"info/info.css"
		],
		"img": [
			"static/logo168.png",
			"static/logo144.png",
			"static/logo192.png",
			"static/logo96.png",
			"static/logo72.png",
			"static/logo48.png"
		]
	},
	"routes": [
		{
			"path": "/",
			"entry": "overview.js",
			"lazyload": [
				"overview-stage-1.js",
				"overview-stage-2.js"
			]
		},
		{
			"path": "/info/",
			"entry": "info.js",
			"lazyload": [

			]
		}
	],
	"bundles": [
		"overview.js",
		"overview-stage-1.js",
		"overview-stage-2.js",
		"info.js"
	],
	"output": {
		"index.html": "index.html",
		"bundles/overview.js": "a.js",
		"bundles/progressive.js": "b.js",
		"bundles/second-view.js": "d.js",
		"../node_modules/@webcomponents/custom-elements/custom-elements.min.js": "c.js",
		"static/*.png": "static/*.png"
	},
	"rename": {
		"bundles/overview.js": "a.js",
		"bundles/progressive.js": "b.js",
		"bundles/second-view.js": "d.js",
		"../node_modules/@webcomponents/custom-elements/custom-elements.min.js": "c.js",
		"<script src=\"classid.js\"></script>": ""
	},
	"addCopyright": [
		"index.html",
		"bundles/overview.js",
		"bundles/progressive.js",
		"bundles/second-view.js"
	]
}